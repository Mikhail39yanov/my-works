{
  "name": "timers-cli",
  "version": "1.0.0",
  "description": "CLI",
  "author": "yanovmikhail <yanovmikhailco@ya.ru>",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "license": "Apache-2.0",
  "scripts": {
    "prettify": "prettier --write",
    "prettify:all": "prettier --write ./src",
    "lint": "npx eslint ./src",
    "lint:all": "npx eslint ./src",
    "format": "npx eslint ./src --fix",
    "__CLI_COMMAND__": "__CLI_COMMAND__",
    "help": "ts-node ./src/index.ts --api --help",
    "signup": "ts-node ./src/index.ts --api signup",
    "login": "ts-node ./src/index.ts --api login",
    "logout": "ts-node ./src/index.ts --api logout",
    "status": "ts-node ./src/index.ts --api ?isActive=",
    "start": "ts-node ./src/index.ts --api ''",
    "stop": "ts-node ./src/index.ts --api stop"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.11",
    "@types/yargs": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^10.1.4",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.2.4",
    "chalk": "^5.2.0",
    "cli-table": "^0.3.11",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "draftlog": "^1.0.13",
    "express": "^4.17.1",
    "inquirer": "^9.1.4",
    "mkdirp": "^2.1.3",
    "nanoid": "^3.1.10",
    "rimraf": "^4.1.2",
    "walk": "^2.3.15",
    "yargs": "^17.6.2"
  },
  "lint-staged": {
    "*.js": [
      "npm run prettify",
      "npm run format",
      "npm run lint"
    ],
    "*.json": [
      "npm run prettify"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
