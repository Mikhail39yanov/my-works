{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "prettier",
    "eslint:recommended",
    // "plugin:node/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "env": {
    "node": true,
    // "jest/globals": true,
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaScript": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-process-exit": 0
    // "node/no-missing-import": true
    // "node/no-unsupported-features/es-syntax": ["error", { "ignores": ["modules"] }]
  },
  "settings": {}
}
